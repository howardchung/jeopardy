{
  "name": "jeopardy",
  "version": "0.1.2",
  "private": true,
  "dependencies": {
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.0.1",
    "ioredis": "^5.4.2",
    "openai": "^5.1.1",
    "papaparse": "^5.5.1",
    "react": "^18.3.1",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.3",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "type": "module",
  "scripts": {
    "server": "node server/server.ts",
    "ui": "vite --host",
    "build": "npm run buildReact && npm run typecheckServer",
    "buildReact": "vite build && npm run typecheck",
    "typecheckServer": "tsc --project server/tsconfig.json",
    "typecheck": "tsc --project src/tsconfig.json --noEmit",
    "dev": "node --watch server/server.ts",
    "prettier": "npx prettier --write .",
    "updateEps": "curl -sSL -O https://github.com/howardchung/j-archive-parser/raw/release/jeopardy.json.gz"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.6"
  }
}
