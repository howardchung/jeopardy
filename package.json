{
  "name": "jeopardy",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "axios": "^0.24.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "ioredis": "^4.28.2",
    "moniker": "^0.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "socket.io": "^4.4.0",
    "socket.io-client": "^4.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run buildServer",
    "buildServer": "tsc --project server/tsconfig.json --outDir buildServer",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "ts-node-dev --respawn --project server/tsconfig.json server/server.ts",
    "server": "node buildServer/server.js",
    "prettier": "prettier --write ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{ts,tsx,js,jsx,css,html,json}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.28.4",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^8.3.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "papaparse": "^5.3.1",
    "prettier": "2.5.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  }
}
