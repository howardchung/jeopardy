{
  "name": "jeopardy",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^1.3.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "ioredis": "^5.3.1",
    "papaparse": "^5.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.4",
    "socket.io": "^4.6.0",
    "socket.io-client": "^4.6.0",
    "vite": "^4.5.0"
  },
  "scripts": {
    "server": "node buildServer/server.js",
    "react": "vite --host",
    "build": "npm run buildReact && npm run buildServer",
    "buildReact": "vite build && npm run typecheck",
    "buildServer": "tsc --project server/tsconfig.json --outDir buildServer",
    "typecheckServer": "tsc --project server/tsconfig.json --noEmit",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "dev": "ts-node-dev --respawn --transpile-only --project server/tsconfig.json server/server.ts",
    "prettier": "prettier --write ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "prettier": "2.8.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
